{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "dist/server/server.js",
  "engines": {},
  "scripts": {
    "postinstall": "npm run build-server:prod && npm run build-client:prod",
    "test": "cross-env NODE_ENV=test mocha tests --ui qunit --recursive --require babel-core/register",
    "build-client": "cross-env BABEL_ENV=client webpack --mode development --watch",
    "build-client:prod": "cross-env BABEL_ENV=client webpack --mode production",
    "build-server": "cross-env BABEL_ENV=node babel src -d dist --copy-files --watch  ",
    "build-server:prod": "cross-env BABEL_ENV=node babel src -d dist --copy-files  ",
    "start": "cross-env DEBUG=server,server:* node dist/server/server.js",
    "stylelint": "stylelint 'resources/sass/**/*.scss'",
    "eslint": "eslint src tests",
    "dev": "npm run build-server & npm run build-client",
    "dev:win": " start /b npm run build-server&& start /b npm run build-client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://tommhuth@github.com/tommhuth/react-boilerplate.git"
